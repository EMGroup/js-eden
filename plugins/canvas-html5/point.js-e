${{
Point = function(x, y) {
  this.x = x;
  this.y = y;
}
}}$;

func Point { ${{
  var x = arguments[0];
  var y = arguments[1];
  return new Point(x, y);
}}$; };

${{
Point.prototype.toString = function() {
  return "{" + this.x + ", " + this.y + "}";
};
}}$;
