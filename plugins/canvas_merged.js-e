	window.Audiovisual = function(name, type, preload) {
		this.name = name;
		this.obsName = root.currentObservableName();
		this.type = type;
		this.preload = preload;
		this.atEnd = false;
		
		Object.defineProperty(this, "duration", {
			enumerable: true,
			get: function () {
				if (this.elements === undefined || !this.elements[0].duration) {
