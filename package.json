{
  "author": "Construit",
  "name": "construit",
  "productName": "construit",
  "description": "Construit, the Making Construals Environment on the web.",
  "version": "2.1.20",
  "homepage": "jseden.dcs.warwick.ac.uk/construit",
  "repository": {
    "type": "git",
    "url": "git://github.com/EMGroup/js-eden.git"
  },
  "scripts": {
    "test": "jest",
    "start": "electron .",
    "construit": "electron .",
    "postinstall": "electron-rebuild -w serialport -f",
    "release": "grunt cssmin; grunt uglify",
    "devserver": "node ./bin/devserver.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=resources/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --icon=resources/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Construit --version-string.FileDescription=Construit --version-string.ProductName=\"Construit\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=resources/icons/png/128x128.png --prune=true --out=release-builds"
  },
  "bin": {
    "construit": "bin/construit"
  },
  "main": "main.js",
  "devDependencies": {
    "connect": "^3.7.0",
    "finalhandler": "^1.1.2",
    "gateway": "~1.0.0",
    "grunt": "^1.3.0",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "~3.0.0",
    "grunt-contrib-uglify": "git://github.com/gruntjs/grunt-contrib-uglify.git#harmony",
    "grunt-gh-pages": "~3.1.0",
    "http": "0.0.0",
    "jest": "^26.6.3",
    "serve-static": "~1.13.2",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony",
    "wd": "^1.11.4"
  },
  "dependencies": {
    "abcjs": "5.6.2",
    "electron": "^4.0.8",
    "electron-rebuild": "^1.11.0",
    "grunt-contrib-uglify-es": "git://github.com/gruntjs/grunt-contrib-uglify.git#harmony",
    "serialport": "^7.1.5",
    "showdown": "^1.9.1"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=6.10"
  },
  "license": "SEE LICENSE IN LICENSE.txt"
}
