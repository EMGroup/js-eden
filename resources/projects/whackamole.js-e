action molescript {score = 0;		#!Number of successful clicks
maxHoles = 4;	#!Number of holes
targHole = 1;	#!Record what the current hole is

#!Define a single hole as a black circle
hole is Circle(holex*100,80,50,"black");
#!Make a list of many holes
holes is hole with holex is 1..maxHoles;

scoreLabel is Text(score,0,0);
moleimg is Image("moleimg",(100*targHole)-40,80-40,100,80,"http://www.wpclipart.com/cartoon/animals/mouse_rat/rodent/mole_cartoon.jpg");
picture is [holes,moleimg,scoreLabel];

#!Add to the score if the image is clicked
when (moleimg_click) score++;

#!Move the mole randomly every second
when (targHole) {
	wait 1000;
	targHole = randomInteger(1, maxHoles);
}}