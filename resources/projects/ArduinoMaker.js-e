button_leg3 is Line(button_x, button_y+button_width, button_x, button_oy+button_width+ard_uno_breaddist/10+3, metal_colour);
button_leg4 is Line(button_x+2*ard_uno_breaddist, button_oy+button_width, button_x+2*ard_uno_breaddist, button_oy+button_width+ard_uno_breaddist/10+3, metal_colour);

button is [button_leg1, button_leg2, button_leg3, button_leg4, button_rect,button_button,button_blob1,button_blob2, button_blob3, button_blob4];


## Temperature Sensor

tempsensor_x is ard_uno_{pin1}["x"];
tempsensor_y is ard_uno_{pin1}["y"];
tempsensor_width is 1.1 * 2 * ard_uno_breaddist;

tempsensor_cx is tempsensor_x + (2 * ard_uno_breaddist)/2;
