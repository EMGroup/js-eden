## LED
# Here is the script to represent an LED.

pin1 = "Bread_E7";
led_pinlength = 25;
led_height is 36 * board_scale;
led_x1 is ard_uno_{pin1}["x"];
led_y1 is ard_uno_{pin1}["y"];
led_base_width is ard_uno_breaddist * 1.3;
led_cathode is Line(led_x1+ard_uno_breaddist, led_y1, led_x1+ard_uno_breaddist, led_y1 - led_pinlength, "silver");
led_anode is Line(led_x1, led_y1, led_x1, led_y1 - led_pinlength, "silver");

led_colour_off is "#b81a1a" if led_colour == "red" else "#329232" if led_colour == "green" else led_colour;
led_colour_on is "#ff5757" if led_colour == "red" else "#57ff57" if led_colour == "green" else led_colour;
led_colour_map is led_colour_off if !led_on else led_colour_on;

led_base is Ellipse(led_x1+ard_uno_breaddist/2, led_y1- led_pinlength, led_base_width, led_base_width*0.7, led_colour);
led_top1 is Ellipse(led_x1+ard_uno_breaddist/2, led_y1 - led_pinlength, led_base_width-3, (led_base_width - 3)*0.7, led_colour_map);
led_top2 is Circle(led_x1+ard_uno_breaddist/2, led_y1 - led_pinlength - led_height, led_base_width-3, led_colour_map, led_colour_map);
led_top3 is Rectangle(led_x1+ard_uno_breaddist/2 - (((led_base_width*2))/2)+3, led_y1 - led_pinlength - led_height, (led_base_width-3)*2, led_height, led_colour_map, led_colour_map);

led is [led_anode,led_cathode, led_base, led_top1, led_top2,led_top3];

