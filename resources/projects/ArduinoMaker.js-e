	tempsensor_y - tempsensor_offset - tempsensor_height,
	tempsensor_width/2, tempsensor_width/3, "black", "#777");
tempsensor_rect is Rectangle(tempsensor_cx - tempsensor_width/2,
	tempsensor_y - tempsensor_offset - tempsensor_height,
	tempsensor_width, tempsensor_height, "black", "#777");

tempsensor_line1 is Line(tempsensor_cx, tempsensor_y,
	tempsensor_cx, tempsensor_y-tempsensor_offset, metal_colour);
tempsensor_line2a is Line(tempsensor_cx-tempsensor_width*0.25,
	tempsensor_y-tempsensor_offset*0.5,
	tempsensor_cx-tempsensor_width*0.25,
	tempsensor_y-tempsensor_offset, metal_colour);

tempsensor_line2b is Line(tempsensor_cx-tempsensor_width*0.25,
	tempsensor_y-tempsensor_offset*0.5,
	tempsensor_x,
	tempsensor_y, metal_colour);

tempsensor_line3a is Line(tempsensor_cx+tempsensor_width*0.25,
	tempsensor_y-tempsensor_offset*0.5,
	tempsensor_cx+tempsensor_width*0.25,
