	wire_x2 = ard_uno_{pin2}["x"],
	wire_y2 = ard_uno_{pin2}["y"];}action components {do @external ResistorColours > page1;
do @external .id(197) >> rgb2colour;

metal_colour = "#999";

## LED

pin1 = "Bread_E7";
led_colour = "red";
led_pinlength = 25;
led_height is 36 * board_scale;
led_x1 is ard_uno_{pin1}["x"];
led_y1 is ard_uno_{pin1}["y"];
led_base_width is ard_uno_breaddist * 1.3;
led_cathode is Line(led_x1+ard_uno_breaddist, led_y1, led_x1+ard_uno_breaddist, led_y1 - led_pinlength, metal_colour);
led_anode is Line(led_x1, led_y1, led_x1, led_y1 - led_pinlength, metal_colour);

interp_col_r is floor($1[1] + ($2[1] - $1[1]) * $3);
interp_col_g is floor($1[2] + ($2[2] - $1[2]) * $3);
interp_col_b is floor($1[3] + ($2[3] - $1[3]) * $3);
interp_colours is rgb2colour(interp_col_r, interp_col_g, interp_col_b);

led_colours_green_on = [87,255,87];
led_colours_green_off = [50,146,50];
led_colours_red_on = [255,87,87];
led_colours_red_off = [184,26,26];

led_status is led_on/256 if type(led_on) == "int" || type(led_on) == "float" else 1.0 if led_on else 0.0;
led_colour_map is interp_colours::(
	led_colours_{led_colour}_off,
	led_colours_{led_colour}_on,
	led_status);

led_base is Ellipse(led_x1+ard_uno_breaddist/2, led_y1- led_pinlength, led_base_width, led_base_width*0.7, led_colour);
led_top1 is Ellipse(led_x1+ard_uno_breaddist/2, led_y1 - led_pinlength, led_base_width-3, (led_base_width - 3)*0.7, led_colour_map);
led_top2 is Circle(led_x1+ard_uno_breaddist/2, led_y1 - led_pinlength - led_height, led_base_width-3, led_colour_map, led_colour_map);
led_top3 is Rectangle(led_x1+ard_uno_breaddist/2 - (((led_base_width*2))/2)+3, led_y1 - led_pinlength - led_height, (led_base_width-3)*2, led_height, led_colour_map, led_colour_map);

led is [led_anode,led_cathode, led_base, led_top1, led_top2,led_top3];

## Resistor

resistor_x1 is ard_uno_{pin1}["x"];
