resistor_wire1 is Curve(resistor_x1, resistor_y1, resistor_x1+resistor_excess,
	resistor_y1-resistor_excess, 4, 0.8225, 0, 0,0.5,0,false, metal_colour);
resistor_wire2 is Curve(resistor_x2, resistor_y2, resistor_x2+resistor_excess,
	resistor_y2+resistor_excess, 4, 0.8225, 0, 0,0.5,0,true, metal_colour);

resistor_dx is resistor_x1 - resistor_x2;
resistor_dy is resistor_y1 - resistor_y2;
resistor_distance is sqrt(resistor_dx * resistor_dx + resistor_dy * resistor_dy);
resistor_excess is (resistor_distance - resistor_length) / 2;

resistor_length is 65 * board_scale;
resistor_width is 25 * board_scale;
resistor_angle is asin(resistor_dx/resistor_distance);

resistor_blob1 is RoundedRectangle(-resistor_width/2,
	0,resistor_width,resistor_width,
	3, "orange");

resistor_blob2 is RoundedRectangle(-resistor_width/2,
	resistor_length-resistor_width,resistor_width,resistor_width,
	3, "orange");

resistor_blob3 is Rectangle(-resistor_width*0.8/2,
	resistor_width*0.8, resistor_width*0.8,resistor_length-2*resistor_width*0.8,
	"orange");

resistor_colour1 is Line(-resistor_width/2, 10*board_scale,
	resistor_width/2, 10*board_scale, valueA_colour);
resistor_colour2 is Line(-resistor_width*0.9/2, 10*2.2*board_scale,
	resistor_width*0.9/2, 10*2.2*board_scale, valueB_colour);
resistor_colour3 is Line(-resistor_width*0.8/2, 10*3.4*board_scale,
	resistor_width*0.8/2, 10*3.4*board_scale, multi_colour);
resistor_colour4 is Line(-resistor_width/2, 10*4.9*board_scale,
	resistor_width/2, 10*4.9*board_scale, tolerance_colour);

resistor_ox is resistor_x2 + resistor_excess + resistor_dx/2;
resistor_oy is resistor_y2 + resistor_excess+resistor_length/2;

/*resistor_angle = 107;*/
resistor_blob_comp is [resistor_blob1,resistor_blob2,resistor_blob3,
	resistor_colour1, resistor_colour2, resistor_colour3, resistor_colour4];
resistor_blob is Translate(0,0,resistor_ox, resistor_oy,RotateAboutCentre(resistor_angle, Point(0, resistor_length/2), Point(0, 0), resistor_blob_comp));

resistor is [resistor_wire1, resistor_wire2, resistor_blob];

## Button

button_x is ard_uno_{pin1}["x"];
button_y is ard_uno_{pin1}["y"];
button_width is ard_uno_breaddist*2 + ard_uno_breaddist/2;
