button_blob3 is Circle(button_ox+button_blob_offset*button_width, button_oy+(1.0-button_blob_offset)*button_width, (button_blob_offset/2)*button_width, "black");
button_blob4 is Circle(button_ox+(1.0-button_blob_offset)*button_width, button_oy+(1.0-button_blob_offset)*button_width, (button_blob_offset/2)*button_width, "black");

button_leg1 is Line(button_x, button_y-(5*board_scale), button_x, button_oy, metal_colour);
