func strplus{
  para symbol;
  if (symbol=="$")
  return "$end";
  else if (symbol=="+")
  return"+";
  else if (symbol=="*")
  return "*";
  else if (symbol=="E")
  return "E";
  else if (symbol=="-")
  return "DIFF";
  else if (symbol==":")
  return "SELECT";
  else if (symbol=="%")
  return "PROJECT";
  else if (symbol==",")
  return "COMMA";
  else if (symbol=="=")
  return "EQ";
  else if (symbol==">")
  return "GT";
  else if (symbol=="<")
  return "LT";
  else if (symbol=="!=")
  return "NEQ";
  else if (symbol=="l")
  return "attr_list";
  else if (symbol=="(")
  return "(";
  else if (symbol==")")
  return ")";
  else if (symbol=="i")
  return "ID";
  else if (symbol=="v")
  return "VAL";
  else if (symbol=="c")
  return "comparator";
  else if (symbol=="a")
  return "ATTRID";
  else if (symbol=="b")
  return "boolexpr";
  else if (symbol=="F")
  return "F";
  else
  return symbol;



}
